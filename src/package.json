{
  "name": "mvcboilerplate",
  "license": "UNLICENSED",
  "description": "Mvc boilerplate. Express + ReactJS. JSPM",
  "repository": "https://github.com/shukriadams/expreactboilerplate",
  "scripts": {
    "start": "node index",
    "build" : "npm run client && npm run ts && node ./build/build-all-sass",
    "client" : "sh ./build/bundle.sh",
    "ts" : "tslint -c tslint.json 'server/**/*.ts' && tsc",
    "dev" : "node ./build/dev.js"
  },
  "devDependencies" : {
    "fileconcat" : "2.0.1",
    "clean-css" : "4.2.1",
    "chokidar" : "2.1.5",
    "node-sass": "4.11.0",
    "cross-spawn": "6.0.5",
    "node-sass-runner": "https://github.com/shukriadams/node-sass-runner.git"
  },
  "dependencies": {
    "fs-extra": "6.0.1",
    "express": "4.16.3",
    "body-parser": "1.18.3",
    "custom-env": "1.0.0",
    "handlebars": "4.0.11",
    "mongodb": "2.2.28",
    "socket.io": "2.1.1",
    "handlebars-layouts": "3.1.4"
  },
  "jspm": {
    "registry": "npm",
    "directories": {
      "baseURL": "client/app",
      "packages": "client/lib"
    },
    "dependencies": {
      "babel": "babel-core@5.8.38",
      "babel-plugin-transform-react-jsx": "6.24.1",
      "babel-runtime": "5.8.38",
      "classnames": "2.2.5",
      "core-js": "2.6.5",
      "history": "4.7.2",
      "localforage": "1.5.6",
      "react": "16.6.3",
      "react-dom": "16.6.3",
      "react-redux": "5.0.6",
      "react-router-dom": "4.2.2",
      "react-router-hash-link": "1.2.0",
      "redux": "3.7.2",
      "redux-persist": "5.7.0",
      "redux-watch": "1.1.1",
      "socket.io-client": "2.1.1"
    }
  }
}
